{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "aymanalhattami/filament-page-with-sidebar": "^2.5",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-language-switch": "^3.1",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "fakerphp/faker": "^1.24",
        "filament/actions": "^3.2",
        "filament/filament": "^3.3",
        "filament/forms": "^3.3",
        "filament/infolists": "^3.3",
        "filament/notifications": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/tables": "^3.3",
        "filament/widgets": "^3.2",
        "guzzlehttp/guzzle": "^7.8",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "jantinnerezo/livewire-alert": "^3.0",
        "laravel/cashier": "^15.6",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.31",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.6.4",
        "moneyphp/money": "^4.5.0",
        "predis/predis": "^3.0",
        "prevailexcel/laravel-nowpayments": "^1.0",
        "ramsey/uuid": "^4.7",
        "reliese/laravel": "^1.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.20",
        "stripe/stripe-php": "^16.6"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "process-timeout": 1800
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
