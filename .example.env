APP_NAME=1000Proxy
APP_ENV=production
VITE_URL=http://your-domain.com
APP_KEY=your-generated-key
APP_DEBUG=true
APP_URL=http://your-domain.com
NOWPAYMENTS_IPN_SECRET=
NOWPAYMENTS_API_KEY=

LOG_CHANNEL=stack
LOG_LEVEL=error

# Database configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
